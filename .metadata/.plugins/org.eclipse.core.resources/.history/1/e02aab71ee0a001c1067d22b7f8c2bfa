		// SKANNERIN LUONTI
		Scanner input = new Scanner(System.in);
		// DECIMALFORMATTERIN LUONTI KAHDEN DESIMAALIN TARKKUUDELLE
		DecimalFormat desimaalit = new DecimalFormat("0.00");
		
		System.out.print("Anna vuositulosi ja ikäsi: ");
		// SEURAAVAKSI PITÄISI LUKEA KAKSI LUKUA , JOTKA SYÖTETÄÄN PERÄKKÄIN VÄLILYÖNTI VÄLISSÄ
		double vuositulo = input.nextDouble();
		int ika = input.nextInt();
		
		double ylevero = 0;
		
		
		System.out.println("Ylevero on ");
		// Anna vuositulosi ja ikäsi: 9000 21
		// Ylevero on 0,00

	}

}
